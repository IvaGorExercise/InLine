@model InLine.UI.Web.T4.FormFormattedTemplate.View.DokumentVrDetaljFormFormatted

 <style type="text/css">

 #Naziv { width:100%;}

</style>


 @{
     string prefixLabela = "labela_DokumentVr_Detalj_";
 }
 

 @{
     string Akcija = @InLine.Model.Data.FieldString.View.DokumentVr.DokumentVrEntity + InLine.UI.Web.Helper.KeyWord.TipAkcija.Insert;
     string IDForma = @InLine.Model.Data.FieldString.View.DokumentVr.DokumentVrEntity + InLine.UI.Web.Helper.KeyWord.Forma.Rijec.Forma;
 }


 @using (Html.BeginForm(Akcija, @InLine.UI.Web.T4.ControllerName.ControllerName.OtkupStanova, FormMethod.Post, new { id = IDForma, @class = InLine.UI.Web.Helper.KeyWord.Forma.Rijec.FormClass }))
 {
    @Html.HiddenFor(model => model.ID_DokumentVr)
    @Html.HiddenFor(model => model.IDDokumentGr)


    @Html.ValidationSummary(true)


<table class ="DokumentVrDetail @InLine.UI.Web.Helper.KeyWord.Forma.Rijec.FormBackground" width="100%" cellspacing="0" cellpadding="0" border="0" style="width:100%;">
   <tbody>

     <tr><td colspan='4'style="height:5px; background-color:#E7E7E7; padding-top: 2px; padding-bottom:2px; "></td></tr>
     
     <tr>
	    <td >@Html.LabelFor(model => model.Oznaka, new { id = @prefixLabela + @Html.NameFor(model => model.Oznaka) })</td>
        <td colspan="3">@Html.TextBoxFor(model => model.Oznaka, new { @disabled = "disabled" })
           @Html.ValidationMessageFor(model => model.Oznaka) </td>				                
    </tr>

     <tr>
	    <td >@Html.LabelFor(model => model.Naziv, new { id = @prefixLabela + @Html.NameFor(model => model.Naziv) })</td>
        <td colspan="3">@Html.EditorFor(model => model.Naziv)
           @Html.ValidationMessageFor(model => model.Naziv) </td>				                
    </tr>

   <tr>
	    <td>@Html.LabelFor(model => model.NazivKr, new { id = @prefixLabela + @Html.NameFor(model => model.NazivKr) })</td>
        <td>@Html.EditorFor(model => model.NazivKr)
        @Html.ValidationMessageFor(model => model.NazivKr)</td>	
        <td>@Html.LabelFor(model => model.Kratica, new { id = @prefixLabela + @Html.NameFor(model => model.Kratica) })</td>
        <td>@Html.EditorFor(model => model.Kratica)
        @Html.ValidationMessageFor(model => model.Kratica)</td>								                
    </tr>

    <tr valign="top">
        <td>
        </td>
        <td >
            <table width="100%">
                <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.ObrocniJednokratni, 2) Jednokratni
                    </td>
                </tr>
                     <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.ObrocniJednokratni, 1)  Obročni
                    </td>
                </tr>
                     <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.ObrocniJednokratni, 0) Niti jedno
                    </td>
                </tr>
            </table>
        </td>

        <td>
            <table width="100%">
                <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.KupacPartner, 1) Kupac
                    </td>
                </tr>
                     <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.KupacPartner, 2) Partner
                    </td>
                </tr>
                     <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.KupacPartner, 0) Niti jedno
                    </td>
                </tr>
            </table>
        </td>

        <td>
         <table width="100%">
                <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.UgovorFakturaNalog, 1) Ugovor
                    </td>
                </tr>
                   <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.UgovorFakturaNalog, 2) Faktura
                    </td>
                </tr>
                        <tr>
                 <td>
                      @Html.RadioButtonFor(model => model.UgovorFakturaNalog, 3) Nalog
                    </td>
                </tr>
                     <tr>
                    <td>
                      @Html.RadioButtonFor(model => model.UgovorFakturaNalog, 0) Niti jedno
                    </td>
                </tr>
            </table>
        </td>

       
							                
    </tr>

    <tr>	
        <td>@Html.LabelFor(model => model.Opis, new { id = @prefixLabela + @Html.NameFor(model => model.Opis) })</td>
        <td colspan="3">@Html.TextAreaFor(model => Model.Opis)
        @Html.ValidationMessageFor(model => model.Opis)</td>								                
    </tr>

 
  </tbody>
</table>
<div>
<div id="poruka_Master" ></div>
</div>

 }

  <script src="@Url.Content("~/Scripts/jquery.jqGrid.addons-4.4.1.js")" type="text/javascript"></script>
